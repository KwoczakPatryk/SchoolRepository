<Group labelreference="L41" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">getRandMat:=proc(modN)
local alpha,chi,i,j;
alpha := Matrix(2);
chi := Matrix(2);

for i from 1 to 2 do
  for j from 1 to 2 do
    alpha[i,j] := getRandVal(0,25000) mod modN;
    chi[i,j] := getRandVal(0,25000) mod modN;
  od;
od;

while chi . MatrixInverse(alpha) = 
        alpha . Chi do
   for i from 1 to 2 do
     for j from 1 to 2 do 
       alpha[i,j] := getRandVal(0,25000);
       chi[i,j] := getRandVal(0,25000);
     od;
   od; 
od;

return alpha,chi;
end:
</Text-field>
</Input>
</Group>
<Group labelreference="L75" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ListToMatrix := proc(myList)
local QBlocks,i,j,m,MatrixQue,MatrixList;
QBlocks := new();
MatrixQue:=new();
for i from 1 to nops(myList) do
  enqueue(QBlocks, myList[i]);
od;

while not empty(QBlocks) do
  m:=Matrix(2); 
  for i from 1 to 2 do
    for j from 1 to 2 do
      if empty(QBlocks) then
        m[i,j] := 0;
      
      else
        m[i,j] := dequeue(QBlocks);

      fi;
    od;
  od;
  enqueue(MatrixQue,m);
od;

MatrixList:=[seq(dequeue(MatrixQue),i=1..length(MatrixQue))];
return MatrixList;

end:</Text-field>
</Input>
