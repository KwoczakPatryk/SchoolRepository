</Input>
</Group>
<Group labelreference="L55" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MatrixToList := proc(myMatrices)
local i,j,n,tempQue,myList;

tempQue:=new();
for n from 1 to nops(myMatrices) do
  for i from 1 to 2 do
    for j from 1 to 2 do
      enqueue(tempQue, myMatrices[n][i,j]);
    od;
  od;
od;

myList:=[seq(dequeue(tempQue),i=1..length(tempQue))];
return myList;

end:</Text-field>
</Input>
</Group>
<Group labelreference="L84" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CayleyPurserEncrypt := proc(msg::string, publicKeys)
local n, alpha, beta, gamma,delta,epsilon,s,kappa,NumberList,MatrixList,EncryptedList;

n:=publicKeys[1];
alpha:=publicKeys[2];
beta:=publicKeys[3];
gamma:=publicKeys[4];

s:=getRandVal(10,100);
delta:=gamma ^ s;
epsilon := MatrixInverse(delta) . alpha . delta;
kappa := MatrixInverse(delta) . beta . delta;

NumberList := StringToList(msg, base = n);

MatrixList := ListToMatrix(NumberList);

#Encryption
EncryptedList:=map(x -&gt; matrixMult(kappa , matrixMult( x , kappa,n),n), MatrixList);

return EncryptedList,epsilon;

end:</Text-field>
</Input>
</Group>
<Group labelreference="L87" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CayleyPurserDecrypt:=proc(encryptedMsg, privateKey, epsilon, n)
local chi, chiInverse, lambda, msgMatrix, msgList, msgString;
chi := privateKey;
chiInverse:=MatrixInverse(chi);

lambda:= chiInverse . epsilon . chi:

#Decryption and conversion to text
msgMatrix:= map(x -&gt; matrixMult(lambda , matrixMult( x , lambda ,n),n), encryptedMsg);
msgList := MatrixToList(msgMatrix);
msgString := ListToString(msgList, base = n);

return msgString;
end:</Text-field>
</Input>
</Group>
