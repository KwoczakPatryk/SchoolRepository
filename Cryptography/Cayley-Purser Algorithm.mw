<Group labelreference="L5" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">matrixAdd:=proc(mat1, mat2, n)
local matSum,i,j;
matSum := mat1 + mat2;
for i from 1 to Dimension(matSum)[1] do
  for j from 1 to Dimension(matSum)[2] do
    matSum[i,j] := matSum[i,j] mod n;
  od;
od;
return matSum;
end:
</Text-field>
</Input>
</Group>
<Group labelreference="L30" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">matrixMult:=proc(mat1,mat2,n)
local mat,i,j;
mat := mat1 . mat2;
for i from 1 to Dimension(mat)[1] do
  for j from 1 to Dimension(mat)[2] do
    mat[i,j] := mat[i,j] mod n;
  od;
od;
return mat;
end:</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L39" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">getKeys:=proc({magnitude::posint:=100})
local p,q,n,alpha,chi,alphaInv,chiInv,beta,r,gamma;
p:=getRandPrime(magnitude);
q:=getRandPrime(magnitude);
n:=p*q;
alpha,chi := getRandMat(n);
alphaInv := MatrixInverse(alpha);
chiInv := MatrixInverse(chi);
beta := chiInv . alphaInv . chi; 
r := getRandVal(50,100);
gamma := chi ^ r;
return [[n,alpha,beta, gamma],chi];
end:
</Text-field>
</Input>
</Group>
